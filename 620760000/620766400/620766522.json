{
  "id": 620766522,
  "txt": [
    {
      "id": "0x624fe1",
      "en": "'So many visitors a-calling!'",
      "es": "'¡Tantos visitantes a la vista!'",
      "es (fem)": ""
    },
    {
      "id": "0x624fe2",
      "en": "'Now don't you be listening to Old Man Willow's singing!'",
      "es": "'¡No escuchéis la canción del Viejo Hombre-Sauce!'",
      "es (fem)": ""
    },
    {
      "id": "0x624fe3",
      "en": "'You are welcome to stay and make merry!'",
      "es": "'¡Podéis quedaros y celebrar!'",
      "es (fem)": ""
    },
    {
      "id": "0x624fe4",
      "en": "'Ho now! Good day, merry friends!'",
      "es": "'¡Hola! ¡Buen día, amigos alegres!'",
      "es (fem)": ""
    }
  ]
}